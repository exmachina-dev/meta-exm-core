From 93099c078b808ba8f8599c0f45623a91cb72293a Mon Sep 17 00:00:00 2001
From: Benoit Rapidel <benoit.rapidel@exmachina.fr>
Date: Tue, 3 Nov 2015 13:57:21 +0100
Subject: [PATCH 2/2] enable uart5

---
 arch/arm/boot/dts/am335x-bone-common.dtsi |   13 +++++++++++++
 1 file changed, 13 insertions(+)

diff --git a/arch/arm/boot/dts/am335x-bone-common.dtsi b/arch/arm/boot/dts/am335x-bone-common.dtsi
index 5bdd880..d629164 100644
--- a/arch/arm/boot/dts/am335x-bone-common.dtsi
+++ b/arch/arm/boot/dts/am335x-bone-common.dtsi
@@ -96,6 +96,13 @@
 		>;
         };
+ 
+        uart5_pins: pinmux_uart5_pins {
+		pinctrl-single,pins = <
+			0x0C0 (PIN_INPUT_PULLUP | MUX_MODE4)	/* uart5_rxd.uart5_rxd */
+			0x0C4 (PIN_OUTPUT_PULLDOWN | MUX_MODE4)	/* uart5_rxd.uart5_rxd */
+		>;
+        };
 
 	clkout2_pin: pinmux_clkout2_pin {
 		pinctrl-single,pins = <
@@ -194,6 +200,13 @@
 	status = "okay";
 };
 
+&uart6 {
+	pinctrl-names = "default";
+	pinctrl-0 = <&uart5_pins>;
+
+	status = "okay";
+};
+
 &usb {
 	status = "okay";
 
